{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "types": ["jquery","bootstrap"],
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strict": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    }
  },
  "compileOnSave": false
}
